{
  "name": "lynx",
  "description": "Modern, lightweight, and planet-scale link shortener + analytics + management!",
  "repository": "JaneJeon/Lynx",
  "author": "Jane Jeon <JaneJeon9719@gmail.com>",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node bin/www",
    "dev": "CI=true yarn build && run-p dev:* watch",
    "dev:www": "DEBUG=knex:query nodemon bin/www | pino-pretty -t",
    "migrate": "knex migrate:latest",
    "migrate:one": "knex migrate:up",
    "migrate:make": "knex migrate:make",
    "migrate:list": "knex migrate:list",
    "rollback": "knex migrate:rollback",
    "rollback:one": "knex migrate:down",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make",
    "build": "run-s migrate build:*",
    "build:yaml": "yaml2json config --save --recursive",
    "build:frontend": "is-ci && echo 'skipping build...' || next build",
    "watch": "run-p watch:*",
    "watch:yaml": "yarn build:yaml --watch",
    "lint": "run-p lint:* --aggregate-output",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check '**/*.{js,md,json,yml,yaml,css,html}'",
    "pretest": "yarn build",
    "test": "cross-env LOG_LEVEL=error jest --forceExit --colors | pino-pretty -t",
    "test:ci": "yarn test --ci --reporters=default --reporters=jest-junit",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@casl/ability": "^3.4.0",
    "@casl/react": "^1.0.4",
    "bootstrap-icons": "^1.0.0-alpha2",
    "connect-ensure-login": "^0.1.1",
    "connect-redis": "^4.0.3",
    "cookie-parser": "^1.4.4",
    "dotenv-defaults": "^1.0.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-query-boolean": "^2.0.0",
    "express-request-id": "^1.4.1",
    "express-session": "^1.16.2",
    "helmet": "^3.21.1",
    "http-errors": "^1.7.3",
    "ioredis": "^4.14.1",
    "knex": "^0.19.5",
    "lodash": "^4.17.15",
    "metascraper": "^5.7.6",
    "metascraper-author": "^5.7.6",
    "metascraper-date": "^5.7.6",
    "metascraper-description": "^5.7.6",
    "metascraper-lang": "^5.7.6",
    "metascraper-logo": "^5.7.6",
    "metascraper-logo-favicon": "^5.7.6",
    "metascraper-publisher": "^5.7.6",
    "metascraper-title": "^5.7.6",
    "ms": "^2.1.2",
    "nanoid": "^2.1.4",
    "next": "^9.0.7",
    "normalize-url": "^4.4.1",
    "objection": "^2.0.10",
    "objection-authorize": "^3.1.1",
    "objection-hashid": "^1.2.0",
    "objection-table-name": "^1.2.0",
    "objection-visibility": "^0.4.0",
    "passport": "^0.4.1",
    "passport-remember-me-extended": "^0.0.3",
    "passport-slack-fixed": "^0.0.8",
    "pg": "^7.12.1",
    "pino": "^5.13.4",
    "pino-pretty": "^3.2.1",
    "pm2": "^4.0.0",
    "react": "^16.10.0",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-dom": "^16.10.0",
    "slack": "^11.0.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "cross-env": "^6.0.0",
    "cypress": "^3.8.2",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.5",
    "is-ci": "^2.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "lint-staged": "^9.3.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "objection-gen": "^0.5.2",
    "objection-swagger": "^3.1.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
