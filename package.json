  "license": "AGPL-3.0",
  "proxy": "http://localhost:3000",
  "scripts": {
    "prestart": "run-s migrate seed --print-name",
    "start": "run-p start:* watch --race",
    "start:frontend": "cross-env PORT=3001 react-scripts start",
    "start:backend": "cross-env PORT=3000 DEBUG=knex:*,ioredis:* nodemon bin/www",
    "migrate": "knex migrate:latest",
    "migrate:one": "knex migrate:up",
    "migrate:make": "knex migrate:make",
    "migrate:list": "knex migrate:list",
    "rollback": "knex migrate:rollback",
    "rollback:one": "knex migrate:down",
    "seed": "knex seed:run",
    "seed:make": "./scripts/make-seed.sh",
    "build": "react-scripts build",
    "clean": "run-p clean:*",
    "clean:frontend": "rimraf build",
    "clean:logs": "rimraf logs/*.log",
    "lint": "run-s lint:* --print-name",
    "lint:prettier": "prettier --check . --ignore-path .gitignore",
    "lint:eslint": "eslint . --cache --ignore-pattern website/",
    "pretest": "cross-env NODE_ENV=test run-s migrate seed --print-name",
    "test": "run-s test:nowatch:* --print-name",
    "test:watch": "npm run pretest && run-s test:watch:* --print-name",
    "test:nowatch:frontend": "cross-env CI=true npm run test:watch:frontend",
    "test:watch:frontend": "react-scripts test",
    "test:nowatch:backend": "jest",
    "test:watch:backend": "npm run test:nowatch:backend --watch"
  },
  "dependencies": {
