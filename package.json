{
  "name": "myurl",
  "version": "0.6.0",
  "description": "A blazing-fast, planet-scale, and easy-to-use serverless URL shortener!",
  "bin": "./bin/deploy.js",
  "repository": "JaneJeon/myURL",
  "author": "Jane Jeon <JaneJeon9719@gmail.com>",
  "license": "AGPL-3.0",
  "scripts": {
    "start": "node app",
    "dev": "yarn build && run-p dev:*",
    "dev:client": "next dev",
    "dev:server": "nodemon app",
    "migrate": "knex migrate:latest",
    "migrate:one": "knex migrate:up",
    "migrate:make": "knex migrate:make",
    "migrate:list": "knex migrate:list",
    "rollback": "knex migrate:rollback",
    "rollback:one": "knex migrate:down",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make",
    "build": "run-p migrate build:*",
    "build:yaml": "yaml2json config --save --recursive",
    "build:client": "if ! is-ci ; then next build && next export; fi",
    "lint": "run-p lint:* --aggregate-output",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check '**/*.{js,md,json,yml,yaml,css,html}'",
    "pretest": "yarn build",
    "test": "cross-env LOG_LEVEL=error run-s 'test:* {@}' -- -- --forceExit",
    "test:unit": "jest models",
    "test:integration": "jest routes",
    "test:watch:all": "yarn test --watch",
    "coverage": "is-ci && codecov",
    "clean": "rimraf logs/*.log website/build .next out"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^3.0.1",
    "connect-ensure-login": "^0.1.1",
    "connect-redis": "^4.0.3",
    "cors": "^2.8.5",
    "dayjs": "^1.8.16",
    "dotenv-defaults": "^1.0.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-prometheus-middleware": "^0.6.1",
    "express-rate-limit": "^5.0.0",
    "express-request-id": "^1.4.1",
    "express-session": "^1.16.2",
    "got": "^9.6.0",
    "grant-express": "^4.6.3",
    "helmet": "^3.21.1",
    "http-assert": "^1.4.1",
    "http-errors": "^1.7.3",
    "ioredis": "^4.14.1",
    "knex": "^0.19.5",
    "lodash": "^4.17.15",
    "mailchecker": "^3.2.37",
    "ms": "^2.1.2",
    "mssql": "^5.1.0",
    "mysql": "^2.17.1",
    "mysql2": "^1.7.0",
    "normalize-email": "^1.1.1",
    "normalize-url": "^4.4.1",
    "objection": "^1.6.11",
    "objection-authorize": "^1.1.0",
    "objection-db-errors": "^1.1.1",
    "objection-hashid": "^1.0.0",
    "objection-table-name": "^1.1.0",
    "objection-visibility": "^0.4.0",
    "passport": "^0.4.0",
    "passport-github2": "^0.1.11",
    "passport-remember-me-extended": "^0.0.3",
    "passport-slack-fixed": "^0.0.8",
    "pg": "^7.12.1",
    "pino": "^5.13.4",
    "pino-pretty": "^3.2.1",
    "pm2": "^4.0.0",
    "rate-limit-redis": "^1.6.0",
    "react-bootstrap": "^1.0.0-beta.14",
    "role-acl": "^4.1.2",
    "sqlite3": "^4.1.0",
    "ua-parser-js": "^0.7.20",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "codecov": "^3.6.1",
    "cross-env": "^6.0.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.5",
    "is-ci": "^2.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "lint-staged": "^9.3.0",
    "morgan": "^1.9.1",
    "next": "^9.0.7",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "objection-gen": "^0.5.2",
    "prettier": "^1.18.2",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "supertest-session": "^4.0.0",
    "surge": "^0.21.3"
  },
  "keywords": [
    "url",
    "shortener",
    "bitly",
    "domain",
    "link",
    "express",
    "aws",
    "cloudfront",
    "cdn",
    "cache",
    "mongo",
    "mongodb",
    "mongoose",
    "react",
    "nextjs"
  ]
}
